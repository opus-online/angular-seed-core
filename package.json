  "description": "",
  "scripts": {
    "build": "babel src --out-dir dist --source-maps inline",
    "lint": "eslint src/**/*.js",
    "precommit": "npm run test && npm run lint",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "main": "dist/index.js",
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "4.4.4",
    "npm": "2.15.1"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 10,
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-register": "^6.11.6",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "husky": "^0.11.4",
    "sinon": "^1.17.5"
  },
  "dependencies": {
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "lodash": "^4.6.1"
  }
}
